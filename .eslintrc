{
  "parser": "@babel/eslint-parser",

  "extends": ["eslint:recommended", "prettier"],

  "plugins": ["prettier"],

  "rules": {
    "prettier/prettier": "error",

    "no-unused-vars": "off"
  },

  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jest": true
  }
}
